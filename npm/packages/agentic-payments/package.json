{
  "name": "@ruvector/agentic-payments",
  "version": "0.1.1",
  "description": "WASM App Store payment integration with agentic-payments (AP2/ACP protocols)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ap2": {
      "import": "./dist/ap2.mjs",
      "require": "./dist/ap2.js",
      "types": "./dist/ap2.d.ts"
    },
    "./acp": {
      "import": "./dist/acp.mjs",
      "require": "./dist/acp.js",
      "types": "./dist/acp.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/ap2.ts src/acp.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "payments",
    "wasm",
    "micropayments",
    "ap2",
    "acp",
    "agentic-payments",
    "app-store",
    "ruvector"
  ],
  "author": "Ruvector Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/ruvector.git",
    "directory": "npm/packages/agentic-payments"
  },
  "homepage": "https://github.com/ruvnet/ruvector#readme",
  "bugs": {
    "url": "https://github.com/ruvnet/ruvector/issues"
  },
  "dependencies": {
    "agentic-payments": "^0.1.13"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.20.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/sdk": {
      "optional": true
    }
  }
}
